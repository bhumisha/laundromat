<div class="modal fade" id="custSignUpModel" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
    aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Sign Up</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form class="signUpForm">
                    <div class="form-group">
                        <label for="signUpNamem">Business Name</label>
                        <input type="text" class="form-control signUpName" id="signUpNamem" autocomplete="Name"
                            aria-describedby="emailHelp" placeholder="Business Name">
                    </div>
                    <div class="form-group">
                        <label for="signUpEmailm">Email address</label>
                        <input type="email" class="form-control signUpEmail" id="signUpEmailm" autocomplete="email"
                            aria-describedby="emailHelp" placeholder="Enter email">
                        <small id="emailHelp" class="">We'll never share your email with anyone
                            else.</small>
                    </div>
                    <div class="form-group">

                        <label for="signUpPwm">Password</label>
                        <input type="password" class="form-control signUpPw" id="signUpPwm" autocomplete="new-password"
                            placeholder="Password">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword1">Re-Enter Password</label>

                        <input type="password" class="form-control pwVerify" id="pwVerifym" autocomplete="new-password"
                            placeholder="Re-Enter Password">
                    </div>
                    <div class="form-group">
                        <label for="signUpStreetAddm">Street Address</label>
                        <input type="text" class="form-control signUpStreetAdd" id="signUpStreetAddm" autocomplete="street-address"
                            aria-describedby="emailHelp" placeholder="Street Address">
                    </div>
                    <div class="form-group">
                        <label for="signUpCitym">City</label>
                        <input type="text" class="form-control signUpCity" id="signUpCitym" autocomplete="city"
                            aria-describedby="emailHelp" placeholder="City">
                    </div>
                    <div class="form-group w-30">
                        <label for="signUpStatem">State</label>
                        <select id="signUpStatem" class="form-select signUpState">
                            {{>states}}
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="signUpZipm">ZIP Code</label>
                        <input type="number" class="form-control signUpZip" id="signUpZipm" autocomplete="postal-code"
                            aria-describedby="emailHelp" placeholder="ZIP">
                    </div>
                    <button type="submit" class="mt-3 btn btn-primary">Sign-Up</button>
                    <button type="button" class="btn mx-3 mt-3 btn-outline-dark btn-rounded" data-bs-dismiss="modal"
                        data-bs-toggle="modal" data-bs-target="#custLoginModel">
                        Login instead!
                    </button>
                </form>
            </div>

        </div>
    </div>
</div>
<script src="/js/cust_signup.js"></script>