<div class="modal fade" id="adminSignUpModel" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
    aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Partner Sign Up</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form action="">
                    <div class="form-group">
                        <label for="signUpNamem">Business Name</label>
                        <input type="text" class="form-control" id="signUpNamem" autocomplete="Name"
                            aria-describedby="emailHelp" placeholder="Business Name">
                    </div>
                    <div class="form-group">
                        <label for="signUpEmailm">Email address</label>
                        <input type="email" class="form-control" id="signUpEmailm" autocomplete="email"
                            aria-describedby="emailHelp" placeholder="Enter email">
                        <small id="emailHelp" class="">We'll never share your email with anyone
                            else.</small>
                    </div>
                    <div class="form-group">
                        <label for="signUpPwm">Password</label>
                        <input type="password" class="form-control" id="signUpPwm" autocomplete="new-password"
                            placeholder="Password">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword1">Re-Enter Password</label>
                        <input type="password" class="form-control" id="pwVerifym" autocomplete="new-password"
                            placeholder="Re-Enter Password">
                    </div>
                    <div class="form-group">
                        <label for="signUpStreetAddm">Street Address</label>
                        <input type="text" class="form-control" id="signUpStreetAddm" autocomplete="street-address"
                            aria-describedby="emailHelp" placeholder="Street Address">
                    </div>
                    <div class="form-group">
                        <label for="signUpCitym">City</label>
                        <input type="text" class="form-control" id="signUpCitym" autocomplete="city"
                            aria-describedby="emailHelp" placeholder="City">
                    </div>
                    <div class="form-group w-30">
                        <label for="signUpStatem">State</label>
                        <select id="signUpStatem" class="form-select ">
                            <option value="AL">AL</option>
                            <option value="AK">AK</option>
                            <option value="AR">AR</option>
                            <option value="AZ">AZ</option>
                            <option value="CA">CA</option>
                            <option value="CO">CO</option>
                            <option value="CT">CT</option>
                            <option value="DC">DC</option>
                            <option value="DE">DE</option>
                            <option value="FL">FL</option>
                            <option value="GA">GA</option>
                            <option value="HI">HI</option>
                            <option value="IA">IA</option>
                            <option value="ID">ID</option>
                            <option value="IL">IL</option>
                            <option value="IN">IN</option>
                            <option value="KS">KS</option>
                            <option value="KY">KY</option>
                            <option value="LA">LA</option>
                            <option value="MA">MA</option>
                            <option value="MD">MD</option>
                            <option value="ME">ME</option>
                            <option value="MI">MI</option>
                            <option value="MN">MN</option>
                            <option value="MO">MO</option>
                            <option value="MS">MS</option>
                            <option value="MT">MT</option>
                            <option value="NC">NC</option>
                            <option value="NE">NE</option>
                            <option value="NH">NH</option>
                            <option value="NJ">NJ</option>
                            <option value="NM">NM</option>
                            <option value="NV">NV</option>
                            <option value="NY">NY</option>
                            <option value="ND">ND</option>
                            <option value="OH">OH</option>
                            <option value="OK">OK</option>
                            <option value="OR">OR</option>
                            <option value="PA">PA</option>
                            <option value="RI">RI</option>
                            <option value="SC">SC</option>
                            <option value="SD">SD</option>
                            <option value="TN">TN</option>
                            <option value="TX">TX</option>
                            <option value="UT">UT</option>
                            <option value="VT">VT</option>
                            <option value="VA">VA</option>
                            <option value="WA">WA</option>
                            <option value="WI">WI</option>
                            <option value="WV">WV</option>
                            <option value="WY">WY</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="signUpZipm">ZIP Code</label>
                        <input type="number" class="form-control" id="signUpZipm" autocomplete="postal-code"
                            aria-describedby="emailHelp" placeholder="ZIP">
                    </div>
                    <button type="submit" class="mt-3 btn btn-primary">Sign-Up</button>
                    <button type="button" class="btn mx-3 mt-3 btn-outline-dark btn-rounded" data-bs-dismiss="modal"
                        data-bs-toggle="modal" data-bs-target="#adminLoginModel">
                        Login instead!
                    </button>
                </form>
            </div>

        </div>
    </div>
</div>
</script>